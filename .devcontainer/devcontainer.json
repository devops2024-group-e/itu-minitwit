{
    "name": "minitwit-dotnet7",
    "image": "mcr.microsoft.com/devcontainers/dotnet:7.0",
    "forwardPorts": [
        8001
    ],
    "postCreateCommand": "sudo sh ./.devcontainer/setup.sh",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-vscode.cpptools-extension-pack",
                "visualstudioexptteam.vscodeintellicode"
            ]
        }
    }
}
